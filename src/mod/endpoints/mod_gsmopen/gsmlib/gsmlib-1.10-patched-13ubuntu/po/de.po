msgid ""
": [-b baudrate][-c concatenatedID][-C sca][-d device][-h][-I init string]\n"
"  [-t][-v][-X] phonenumber [text]"
msgstr ""
": [-b Baud-Rate][-c concatenatedID][-C sca][-d Gerät][-h][-I Init String]\n"
"  [-t][-v][-X]  Telefonnummer [Text]"

#: apps/gsmsendsms.cc:156
